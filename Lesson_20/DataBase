
CREATE TABLE IF NOT EXISTS HomeWork(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    first_name TEXT NOT NULL ,
    second_name TEXT NOT NULL ,
    age INTEGER
    )


CREATE UNIQUE INDEX full_name ON HomeWork(first_name, second_name)

